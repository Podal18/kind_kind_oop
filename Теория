class qq:
    name = "Tolya"
    age = 19

a = qq() #коллекция(объект), {}
setattr(qq, 'salary', 70000.00) #Задает значение, создает атрибут класса/объекта
getattr(qq, 'age', ['True/False']) #19, выводит значение
hasattr(qq, 'Post') #false, т.к. проверяет есть ли этот атрибут в классе
delattr(qq, 'name') #удаление

a.__dict__ #вывод содержимого класса/объекта
a.__doc__ #вывод описания класса


